.bolg_card {
	max-height: 415px;
	height: 100%;
	min-height: 415px;
	max-width: 320px;
	width: 100%;
	display: block;
	margin: 25px;
	border-radius: 10px;
	border-width: 1px;
	border-style: solid;
	border-color: #e07f4c;
	background-color: #f8f8f9;
	overflow: hidden;
	position: relative;
}

.bolg_card:hover {
	-webkit-animation: none;
	        animation: none;
}

.bolg_card:hover .blog_card-des {
	z-index: 5;
	border-top-left-radius: 10px;
	top: 0;
	left: 0;
	transition: all 0.5s ease;
}

.bolg_card:not(:hover) .blog_card-des {
	-webkit-animation: none;
	        animation: none;
	opacity: 0;
}

.bolg_card.other-articalte {
	max-width: 29%;
	margin-left: 0%;
	margin-right: 2%;
	min-height: 390px;
}

.blog_card-profile-details {
	background: #fff;
	box-shadow: 0 -4px 7px 0 rgba(0, 0, 0, 0.1);
	position: absolute;
	bottom: 0;
	width: calc(100% - 26px);
	right: 0;
	padding: 13px 15px 10px 15px;
	z-index: 2;
	border-top-left-radius: 10px;
}

.blog_card-title {
	font-size: 22px;
	color: #2A2A2A;
	line-height: 1.3;
	font-weight: 400;
}

.blog_card-title.title-height {
	min-height: 84px;
	overflow: hidden;
}

.blog_card-posted-by {
	font-size: 20px;
	color: #2A2A2A;
	text-align: right;
	line-height: 1;
	margin-top: 15px;
}

.blog_card-posted-by .date {
	font-size: 14px;
	padding-top: 10px;
}

.blog_card-image {
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	z-index: 1;
	border-top-left-radius: 10px;
}

.blog__main-page {
	max-width: 1110px;
	width: 100%;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	margin: 40px auto 0px auto;
}

.blog__main-page .bolg_card:nth-child(n) {
	border-color: #f85656;
}

.blog__main-page .bolg_card:nth-child(2n) {
	border-color: #02bef4;
}

.blog__main-page .bolg_card:nth-child(3n) {
	border-color: #05cd92;
}

.blog__main-page .bolg_card:nth-child(4n) {
	border-color: #71cd05;
}

.blog__main-page.title {
	-webkit-justify-content: start !important;
	    -ms-flex-pack: start !important;
	        justify-content: start !important;
}

.blog__main-page.title .section-title__text {
	font-size: 34px;
	line-height: 1;
	font-family: "Lato";
}

.blog__main-page.title .section-title__text span {
	word-spacing: 0;
}

.other-articaltes {
	max-width: 1110px;
	width: 100%;
	margin: 0 auto;
	position: relative;
	top: -50px;
}

.other-articaltes .blog__main-page {
	margin: 0;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}

.blog_card-des {
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	position: absolute;
	top: 284px;
	left: 26px;
	background-color: #fff;
	z-index: 0;
	width: 100%;
	height: 100%;
	padding: 20px;
	opacity: 1;
	transition: all 0.3s ease;
}

.blog_card-des .arrow__btn-link {
	-webkit-justify-content: flex-end;
	    -ms-flex-pack: end;
	        justify-content: flex-end;
}

.blog_card-content {
	font-size: 20px;
	color: #2A2A2A;
	line-height: 1.5;
	margin-top: 15px;
}

